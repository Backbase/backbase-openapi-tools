{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "javaType":"com.backbase.buildingblocks.test.data.XmlContent",
  "properties": {
    "id": {
      "type": "string"
    },
    "content": {
      "type": "xml",
      "description": "contents of the request"
    },
    "creationDate" : {
      "type":"string",
      "format":"date-time"
    },
    "user" : {
      "type" : "string",
      "default" : "GUEST"
