    {{/groupId}}
  },
  "keywords": [
    "backbase",
    "web",
    "wa3",
    "wc3",
    "openapi-mock",
    "openapi-generator"
  ],
  "license": "Unlicense",
  "scripts": {
    "build": "ng-packagr -p ng-package.json"
  },
