{
  "name": "{{{npmName}}}",
  "version": "{{{npmVersion}}}",
  "description": "OpenAPI client for {{{npmName}}}",
  "backbase": {
    {{#specVersion}}
    "specVersion": "{{{specVersion}}}"
    {{/specVersion}}
    {{#artifactId}}
    "artifactId": "{{{artifactId}}}"
    {{/artifactId}}
    {{#groupId}}
