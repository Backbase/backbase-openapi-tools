{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "properties": {
    "dataAccessGroupId": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "legalEntityId": {
      "type": "string",
      "required": true
    },
    "currency": {
      "javaType": "com.backbase.presentation.transaction.Money",
      "type": "object",
      "required": false
    },
    "type": {
      "type": "string",
      "required": true
    },
    "subtype": {
      "type": "string",
      "required": true
    },
    "items": {
      "type": "array",
      "required": true,
      "items": {
        "type": "string"
      }
    }
  }
}